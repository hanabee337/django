From ad37a77c3b76e979781e1c98ece90c26a12cd8f5 Mon Sep 17 00:00:00 2001
From: hanabee337 <hanabee337@gmail.com>
Date: Mon, 13 Feb 2017 14:29:14 +0900
Subject: [PATCH 3/3] staticmethod

Signed-off-by: hanabee337 <hanabee337@gmail.com>
---
 django_app/instagram/settings.py |  1 +
 django_app/member/models.py      | 17 +++++++++++++++++
 2 files changed, 18 insertions(+)

diff --git a/django_app/instagram/settings.py b/django_app/instagram/settings.py
index 8d8aa04..51a5e14 100644
--- a/django_app/instagram/settings.py
+++ b/django_app/instagram/settings.py
@@ -41,6 +41,7 @@ INSTALLED_APPS = [
     'django.contrib.messages',
     'django.contrib.staticfiles',
 
+    'django_extensions',
     'member.apps.MemberConfig',
 ]
 
diff --git a/django_app/member/models.py b/django_app/member/models.py
index 24594ea..39dec53 100644
--- a/django_app/member/models.py
+++ b/django_app/member/models.py
@@ -24,6 +24,7 @@ class MyUser(models.Model):
     def __str__(self):
         return self.username
 
+    # shell에서 일일이 작업하는 게 귀찮으니...
     @staticmethod
     def create_dummy_user(num):
         """
@@ -49,3 +50,19 @@ class MyUser(models.Model):
                 print(e)
 
         return created_count
+
+    # shell에서 u1 = MyUser.objects.get(id=1)을 일일이 작업하는 게 귀찮으니...
+    @staticmethod
+    def assign_global_variables():
+        # interpreter를 다루는 부분
+        # sys는 python 인터프리터 관련 내장 모듈
+        import sys
+
+        # __mmain__ 모듈을 module변수에 할당
+        # shell에서 입력한 global 변수와 같은 역할
+        module = sys.modules['__main__']
+        # MyUser 객체 중 'User'로 시작하는 객체들만 조회하여 users변수에 할당
+        users = MyUser.objects.filter(username__startswith='User')
+        # users를 순회하며
+        for index, user in enumerate(users):
+            setattr(module, 'u{}'.format(index + 1), user)
-- 
